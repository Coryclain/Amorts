    padding: 30, 20
    spacing: 50, 15
    Label:
        size_hint: 1,None
        height: 50
        text: "Inventarios"
        color: 0,0,0,1
        pos_hint: {'center_x':0.5,'center_y':0.950}


    BoxLayout:
        data: ''
        data_id: ''
        Button:
            size_hint: 0.9, 0.5
            size_font: self.width*0.4
            #text: app.root.data
            pos_hint: {'center_y':0.850}
            on_release:
        Button:
            size_hint: 0.3, 0.5
            text:'editar'
            pos_hint: {'center_y':0.850}
